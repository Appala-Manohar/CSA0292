
#include <stdio.h>

int main() 
{
    int a[3][3]={9,8,4,8,9,8,4,1,2};
    int tran[3][3]={0,0,0,0,0,0,0,0,0};
    int i,j;
    for (i=0;i<3;i++)
    {
        for (j=0;j<3;j++)
        {
            tran[j][i]=a[i][j];
        }
    }
    for (i=0;i<3;i++)
    {
        for (j=0;j<3;j++)
        {
            printf("%d\t",tran[j][i]);
        }
        printf(" \n");
    }
    return 0;
}
